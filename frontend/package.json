{
  "name": "logon-frontend",
  "version": "1.0.0",
  "description": "Frontend pour LogOn Password Manager - Interface utilisateur sécurisée",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --host 0.0.0.0",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint --ext .ts,.vue .",
    "lint:fix": "eslint --ext .ts,.vue . --fix",
    "format": "prettier --write .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "typecheck": "nuxt typecheck"
  },
  "keywords": [
    "password-manager",
    "nuxt",
    "vue",
    "typescript",
    "security",
    "cryptography"
  ],
  "author": "LogOn Team",
  "license": "MIT",
  "dependencies": {
    "@nuxt/devtools": "latest",
    "@pinia/nuxt": "^0.5.1",
    "@vueuse/nuxt": "^10.5.0",
    "nuxt": "^3.8.0",
    "pinia": "^2.1.7",
    "vue": "^3.3.6",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^3.8.0",
    "@vue/test-utils": "^2.4.2",
    "@nuxt/eslint-config": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "eslint-plugin-vue": "^9.18.1",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6",
    "jsdom": "^22.1.0",
    "@vitest/ui": "^0.34.6",
    "@vitest/coverage-v8": "^0.34.6",
    "happy-dom": "^12.10.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
